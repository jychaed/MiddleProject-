<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
	
	
	<sqlMap namespace = "room_reservation">
	
	<update id="room_reservation_insert" parameterClass="RoomResVo">
		insert into roomres values (
			('roomres'||room_res_seq.nextval), 
			#member_id#, 
			#room_code#, 
			0, 
			#roomres_start#, 
			#roomres_end# 
		)
		
	</update>
	 
	
		
	</sqlMap>